@Library('mylibrary')_
pipeline
{
    agent any
    stages
    {
        stage('contDownload_Master')
        {
            steps
            {
                script
                {
                    cicd.gitDownload("maven")
                }
            }
        }
        stage('contBuild_Master')
        {
            steps
            {
                script
                {
                    cicd.buildArtifact()
                }
            }
        }
        stage('contDeployment_Master')
        {
            steps
            {
                script
                {
                    cicd.deployTomcat("Declarativepipelinewithlibraries","172.31.30.74","testapp")
                }
            }
        }
        stage('contTesting_Master')
        {
            steps
            {
                script
                {
                  cicd.gitDownload("FunctionalTesting")
                  cicd.runSelenium("Declarativepipelinewithlibraries")
                }
            }
        }
        stage('contDelivery_Master')
        {
            steps
            {
                script
                {
                     cicd.deployTomcat("Declarativepipelinewithlibraries","172.31.28.139","prodapp")
                }
            }
        }
    }
}
