@Library('mylibrary')_
pipeline
{
    agent any
    stages
    {
        stage('contDownload_Loans')
        {
            steps
            {
                script
                {
                    cicd.gitDownload("maven")
                }
            }
        }
        stage('contBuild_Loans')
        {
            steps
            {
                script
                {
                    cicd.buildArtifact()
                }
            }
        }
      }
   }
